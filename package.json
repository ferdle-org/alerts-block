  "private": true,
  "type": "module",
  "scripts": {
    "build": "yarn i18n:load && next build",
    "clean:install": "rm -rf node_modules && yarn cache clean && yarn install --offline",
    "dev": "next dev | pino-pretty -a @timestamp -m message",
    "dev:debug": "NODE_OPTIONS='--inspect=0.0.0.0:31000' next dev | pino-pretty -a @timestamp -m message",
    "i18n:convert": "./scripts/convert-locales.js",
    "i18n:download": "download-locales --destination='./i18n/translations' --packages='login_service' --locales='./i18n/locales.json'",
    "i18n:load": "yarn i18n:download && yarn i18n:convert",
    "i18n:yaml": "./scripts/convert-local-yaml.js",
    "lint": "yarn test:lint && yarn test:types",
    "start": "next start",
    "test": "jest --verbose --config ./jest.config.js",
    "test:all": "yarn test && yarn test:lint && yarn test:types",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:lint": "next lint",
    "test:watch": "jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage --passWithNoTests --config ./jest.config.js --verbose"
  },
